######

ATSHOMEQ="$(ATSHOME)"
ATSCC=$(ATSHOMEQ)/bin/atscc
ATSOPT=$(ATSHOMEQ)/bin/atsopt
ATSDOC=$(ATSHOMEQ)/bin/atsdoc

######

PATSHOMEQ="$(PATSHOME)"
INCLUDE=-I $(PATSHOMEQ)/src
INCLATS=-IATS $(PATSHOMEQ) -IATS $(PATSHOMEQ)/src

######

CAT=cat

######

all:: all_dbook

######

DECLATEXT= \
  declatext_sats.o \
  declatext_dats.o \

declatext_sats.o: \
  declatext.sats ; $(ATSCC) $(INCLUDE) $(INCLATS) -c $(ATSCCFLAGS) $<
declatext_dats.o: \
  declatext.dats ; $(ATSCC) $(INCLUDE) $(INCLATS) -c $(ATSCCFLAGS) $<

######

DBOOKGENDECL= \
  dbookgendecl_sats.o \
  dbookgendecl_dats.o \

dbookgendecl_sats.o: \
  dbookgendecl.sats ; $(ATSCC) $(INCLUDE) $(INCLATS) -c $(ATSCCFLAGS) $<
dbookgendecl_dats.o: \
  dbookgendecl.dats ; $(ATSCC) $(INCLUDE) $(INCLATS) -c $(ATSCCFLAGS) $<

dbookgendecl_atxt.dats: dbookgendecl.atxt ;   $(ATSDOC) --outcode $@ -i $< > dbookgendecl_atxt.txt

######

all_dbook:: DBOOK/arith_prf.dbk
DBOOK/arith_prf.dbk:   arith_prf_atxt.exe ; $(CAT) ../arith_prf.sats | ./$< > $@
arith_prf_atxt.txt: arith_prf.atxt ; $(CAT) $< |   $(ATSDOC) --prefix __datatok --outcode dbookgendecl_data_atxt.dats > $@
arith_prf_atxt.exe: dbookgendecl_atxt.dats   arith_prf_atxt.txt $(DECLATEXT) $(DBOOKGENDECL) ;   $(ATSCC) $(INCLUDE) $(INCLATS) -D_ATS_GCATS -o $@ $<     $(DECLATEXT) $(DBOOKGENDECL) $(PATSHOME)/libatsyntax/libatsyntax.a -latsdoc
clean:: ; $(RMF) arith_prf_atxt.exe
cleanall:: ; $(RMF) DBOOK/arith_prf.dbk


